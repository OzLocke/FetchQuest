            <kind>1</kind>
            <string>
///Change page variable and return controller script
if(room == rm_startScreen){
    room_goto(rm_main);
} else if(target = "Quit"){
        room_goto(rm_startScreen);
} else {

    //---Get new items---
    if(inventoryGet != "NOTHING"){
    
        //Check for item in inventory and add it if it's not there
        ini_open(working_directory + "Narrative_Files/inv.ini");
        if(ini_read_real(inventoryGet,"found", "") == 0){
            ini_write_real(inventoryGet, "found", 1); 
        };
        ini_close();
    };
    
    //---Destroy items---
    //If a button includes a call to destroy an item it'll have the real number
    // id of the item, otherwise it'll say NOTHING
    if(inventoryItemDestroy != "NOTHING"){
        ini_open(working_directory + "Narrative_Files/inv.ini");
        //If the item is marked destroy, but hasn't been found yet, don't destroy it
        if(ini_read_real(inventoryItemDestroy, "found", "") == 1){
            ini_write_real(inventoryItemDestroy, "destroyed", 1);
        };
        ini_close();
    };
    
    
    //---Check for required items---
    if(inventoryNeed != "NOTHING"){
    
        //Check for required item and reset target if it's not there
        ini_open(working_directory + "Narrative_Files/inv.ini");
        if(ini_read_real(inventoryNeed, "found", "") == 0){
            target = inventoryMissingTarget;
        };
        ini_close();
    };
    
    //---Set new page---
    scr_controller(target);
};



